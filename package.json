{
  "name": "learning-react",
  "version": "0.0.1",
  "main": "index.js",
  "description": "React/Redux with Server Side Rendering Demo Application",
  "keywords": [
    "react",
    "redux",
    "universal",
    "isomorphic",
    "server side render"
  ],
  "author": "Nicola Baldi",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:server && npm run build:assets",
    "build:server": "npm install && node_modules/.bin/babel src --out-dir build --copy-files",
    "build:assets": "npm install && node_modules/.bin/webpack --progress --color -p --config webpack.config.dev.js",
    "launch": "npm run build && node build"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "path": "^0.12.7",
    "webpack": "^3.8.1"
  },
  "dependencies": {
    "express": "^4.16.2",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2"
  }
}
